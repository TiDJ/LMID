{
  "manifest_version": 2,

  "name" :"Launch My ID",
  "description" :"Launch your 3 favorites urls easily and fastly :) ",
  "version" : "1.0",
  "icons": {
     "128": "md128.png",
     "16": "md16.png",
     "24": "md24.png",
     "32": "md32.png",
     "48": "md48.png",
     "64": "md64.png"
  },
  "browser_action" :{
    "default_icon": "md64.png",
    "default_popup" :"popup.html",
    "default_title": "Click here to fast launch!"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "Toggle feature foo"
    }
  }
}
